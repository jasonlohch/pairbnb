<h1> Create a New Listing</h1>

<%= form_for :listing, url: listings_path do |f| %>
<p>
	<%= f.label :title %><br>
	<%= f.text_field :title %>
</p>

<p>
	<%= f.label :location %><br>
	<%= f.text_field :location %>
</p>

<p>
	<%= f.label :description %><br>
	<%= f.text_area :description %>
</p>

<p>
	<%= f.submit %>
</p>
<% end %>